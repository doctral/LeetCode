public class Solution {
    public int MaxArea(int[] height) {
        int left=0, right=height.Length-1, res=0;
        while(left<right){
            int area=Math.Min(height[left], height[right])*(right-left);
            if(res<area) res=area;
            if(height[left]<height[right]){
                left++;
            } 
            else{
                right--;
            }
        }
        return res;
    }
}